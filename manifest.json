{
    "manifest_version": 2,
    "name": "Schulquiz.info Solver",
    "version": "1.1.1",
    "description": "Easily solve every quiz on schulquiz.info!",
    "content_scripts": [
        {
            "matches": ["https://schulquiz.info/*"],
            "js": ["js/solve.js"],
            "css": ["css/background.css"]
        }
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}